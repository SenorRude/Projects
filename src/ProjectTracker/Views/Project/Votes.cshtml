@model Project

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Votes";
}

<h3 class="list-title">
    @Html.ActionLink("Project Ideas", "Index")
    > @HtmlBuilder.DetailsLink(Url, "Project", Model.Project_Id, Model.Title, null, true)
</h3>
@if (Model.Voters.Count == 0)
{
    @Html.Raw("No one has voted for this project yet.")
}
else
{
    <table class="table table-striped table-hover table-condensed">
        <thead>
            <tr>
                <th>Votes</th>
            </tr>
        </thead>

        @foreach (var item in Model.Voters)
        {
            <tr>
                <td>@HtmlBuilder.DetailsLink(Url, "Employee", item.Emp_Id, item.FullName)</td>
            </tr>
        }
    </table>
}