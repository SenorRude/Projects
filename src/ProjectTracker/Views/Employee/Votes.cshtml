@model Employee

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Votes";
}

<h3 class="list-title">
    @Html.ActionLink("Employees", "Index")
    > @HtmlBuilder.DetailsLink(Url, "Employee", Model.Emp_Id, Model.FullName, null, true)
</h3>
@if (Model.VotedProjects.Count == 0)
{
    @Html.Raw("This user hasn't voted for any projects yet.")
}
else
{
    <table class="table table-striped table-hover table-condensed">
        <thead>
            <tr>
                <th>Votes</th>
            </tr>
        </thead>

        @foreach (var item in Model.VotedProjects)
        {
            <tr>
                <td>@HtmlBuilder.DetailsLink(Url, "Project", item.Project_Id, item.Title)</td>
            </tr>
        }
    </table>
}